{
    "name": "Caption_seq2seq_onehot1024_h512_wd2e-2_GRU",
    "task": "caption",
    "cuda": true,
    "embedder": "OneHotEmbedder",
    "data_loader": {
        "data_dir": "datasets/",
        "batch_size": 16,
        "emb_size": 1024,
        "shuffle": true
    },
    "validation": {
        "validation_split": 0.1,
        "shuffle": true
    },
    "optimizer_type": "RMSprop",
    "optimizer": {
        "lr": 0.001,
        "weight_decay": 1e-06
    },
    "loss": "cross_entropy",
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_freq": 20,
        "verbosity": 2,
        "monitor": "val_loss",
        "monitor_mode": "min"
    },
    "model": {
        "rnn_type": "LSTM",
        "input_size": 4096,
        "hidden_size": 256,
        "emb_size": 1024,
        "layers": 2,
        "enc_dropout": 0.2,
        "dec_dropout": 0.2,
        "bidirectional": false,
        "attention": false,
        "teacher_forcing": false,
        "scheduled_sampling": false,
        "beam_search": false
    }
}
